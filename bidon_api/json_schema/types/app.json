{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "App Schema",
  "type": "object",
  "required": [
    "bundle",
    "key",
    "framework",
    "version"
  ],
  "properties": {
    "bundle": {
      "title": "The bundle Schema",
      "type": "string"
    },
    "key": {
      "title": "The key Schema",
      "type": "string"
    },
    "framework": {
      "title": "The framework Schema",
      "type": "string"
    },
    "version": {
      "title": "The version Schema",
      "type": "string"
    },
    "framework_version": {
      "title": "The framework_version Schema",
      "type": "string"
    },
    "plugin_version": {
      "title": "The plugin_version Schema",
      "type": "string"
    },
    "skadn": {
      "title": "An array of SKAdNetworkItems (ids) from an app's info.plist, only iOS",
      "type": "array",
      "items": {
        "title": "SKAdNetwork id",
        "type": "string"
      }
    }
  }
}
