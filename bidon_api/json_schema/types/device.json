{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Device Schema",
  "type": "object",
  "required": [
    "ua",
    "make",
    "model",
    "os",
    "osv",
    "hwv",
    "h",
    "w",
    "ppi",
    "pxratio",
    "js",
    "language",
    "connection_type",
    "type"
  ],
  "properties": {
    "geo": { "$ref": "geo.json" },
    "ua": {
      "title": "The ua Schema",
      "type": "string"
    },
    "make": {
      "title": "The make Schema",
      "type": "string"
    },
    "model": {
      "title": "The model Schema",
      "type": "string"
    },
    "os": {
      "title": "The os Schema",
      "type": "string"
    },
    "osv": {
      "title": "The osv Schema",
      "type": "string"
    },
    "hwv": {
      "title": "The hwv Schema",
      "type": "string"
    },
    "h": {
      "title": "The h Schema",
      "type": "integer"
    },
    "w": {
      "title": "The w Schema",
      "type": "integer"
    },
    "ppi": {
      "title": "The ppi Schema",
      "type": "integer"
    },
    "pxratio": {
      "title": "The pxratio Schema",
      "type": "number"
    },
    "js": {
      "title": "The js Schema",
      "type": "integer"
    },
    "language": {
      "title": "The language Schema",
      "type": "string"
    },
    "carrier": {
      "title": "The carrier Schema",
      "type": "string"
    },
    "mccmnc": {
      "title": "The mccmnc Schema",
      "type": "string"
    },
    "connection_type": {
      "title": "The connection_type Schema",
      "type": "string",
      "enum": ["ETHERNET", "WIFI", "CELLULAR", "CELLULAR_UNKNOWN" ,"CELLULAR_2_G", "CELLULAR_3_G", "CELLULAR_4_G", "CELLULAR_5_G"]
    },
    "type": {
      "title": "The type Schema",
      "type": "string",
      "enum": ["PHONE", "TABLET"]
    }
  }
}
