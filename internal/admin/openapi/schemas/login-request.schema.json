{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "login-request.schema.json",
  "title": "LogInRequest",
  "type": "object",
  "properties": {
    "email": {
      "type": "string",
      "format": "email",
      "description": "User's email address",
      "example": "user@example.com"
    },
    "password": {
      "type": "string",
      "format": "password",
      "description": "User's password",
      "example": "mypassword"
    }
  },
  "required": ["email", "password"]
}
