{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "error.schema.json",
  "title": "Error",
  "type": "object",
  "required": ["error"],
  "properties": {
    "error": {
      "type": "object",
      "required": ["code", "message"],
      "properties": {
        "code": {
          "type": "integer",
          "description": "Error code"
        },
        "message": {
          "type": "string",
          "description": "Error message"
        }
      }
    }
  }
}
